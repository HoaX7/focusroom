{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "focusroom",
	"compatibility_date": "2025-09-02",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"main": "@tanstack/solid-start/server-entry",
  "observability": {
    "enabled": true
  },
  "routes": [{
    "pattern": "focusroom.dev",
    "custom_domain": true
  }],
  "workers_dev": false,
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "focusroom",
			"database_id": "",
			"migrations_dir": "migrations",
			"migrations_table": "migrations"
		}
	]
}